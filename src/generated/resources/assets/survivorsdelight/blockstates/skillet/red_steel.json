{
  "multipart": [
    {
      "apply": {
        "model": "survivorsdelight:block/skillet/red_steel"
      },
      "when": {
        "facing": "north"
      }
    },
    {
      "apply": {
        "model": "survivorsdelight:block/skillet/red_steel",
        "y": 180
      },
      "when": {
        "facing": "south"
      }
    },
    {
      "apply": {
        "model": "survivorsdelight:block/skillet/red_steel",
        "y": 90
      },
      "when": {
        "facing": "east"
      }
    },
    {
      "apply": {
        "model": "survivorsdelight:block/skillet/red_steel",
        "y": 270
      },
      "when": {
        "facing": "west"
      }
    },
    {
      "apply": {
        "model": "survivorsdelight:block/skillet/tray"
      },
      "when": {
        "support": "true"
      }
    }
  ]
}